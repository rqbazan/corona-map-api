{
  "extends": ["airbnb-base", "prettier"],
  "parser": "babel-eslint",
  "settings": {
    "import/resolver": {
      "babel-module": {}
    }
  },
  "rules": {
    "no-console": "off",
    "import/prefer-default-export": "off",
    "max-classes-per-file": "off",
    "class-methods-use-this": "off",
    "no-unused-vars": ["error", { "varsIgnorePattern": "I" }]
  }
}
